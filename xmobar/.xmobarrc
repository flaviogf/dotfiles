Config { font = "xft:JetBrains Mono Nerd Font:regular:pixelsize=11"
       , additionalFonts = ["xft:Font Awesome 6 Free Solid:pixelsize=12"]
       , bgColor = "#282a36"
       , fgColor = "#f8f8f2"
       , position = TopSize L 100 24
       , commands = [ Run Cpu ["-t", "<fn=1>\xf2db</fn> cpu: <total>%", "-H", "50", "--high", "red"] 50
                    , Run Memory ["-t", "<fn=1>\xf233</fn> mem: <usedratio>%"] 50
                    , Run Date "<fn=1>\xf133</fn> %m/%d/%y %H:%M" "date" 50
                    , Run BatteryP ["BAT1"] ["-t", "<fn=1>\xf240</fn> bat: <left>%"] 50
                    , Run UnsafeStdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%UnsafeStdinReader% }{ <box type=Bottom width=2 color=#f1fa8c><fc=#f1fa8c>%cpu%</fc></box>  <box type=Bottom width=2 color=#ff5555><fc=#ff5555>%memory%</fc></box>  <box type=Bottom width=2 color=#5af78e><fc=#5af78e>%battery%</fc></box>  <box type=Bottom width=2 color=#caa9fa><fc=#caa9fa>%date%</fc></box> "
       }
